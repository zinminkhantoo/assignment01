<?php
 function solution($a, $b , $c) {
     $t=0;
     $b=2;
     $a=3;
     $c=4;
     $x=0;
     $y=0;
     $t=(pow($b,2))-(4*$a*$c);
     
     if($t<=0)
         $t=(-1)*$t;
     
         $x=(-2*$c)/($b+(sqrt($t)));
         $y=(-2*$c)/($b-(sqrt($t)));
 }
 
 echo"Solution of (1,0,-1) is: ";
?>