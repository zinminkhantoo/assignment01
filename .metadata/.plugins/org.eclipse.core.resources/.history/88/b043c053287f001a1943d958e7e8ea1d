<?php
    define(TIME, 30/*min*/);
    define(WAGE, 20/*$*/);
    function Programmer($p1, $p2, $p3) {
        $p1 = ($p1/TIME)*WAGE;
        $p2 = ($p2/TIME)*WAGE;
        $p3 = ($p3/TIME)*WAGE;
        $high = 0;
        $low = 0;
        switch ($high) {
            case $p1>$p2: $high = $p1;break;
            case $p1>$p3: $high = $p1;break;
            case $p2>$p1: $high = $p2;break;
            case $p2>$p3: $high = $p2;break;
            case $p3>$p1: $high = $p2;break;
            case $p3>$p2: $high = $p2;break;
            
            default:
                ;
            break;
        }
        
    }
?>