<?php
    class Cart {
        private $id;
        private $name;
        private $price;
        function __construct($id, $name, $price) {
            $this->id = $id;
            $this->name = $name;
            $this->price = $price;
        }
        function GetID() {
            return $this->id;
        }
        function GetName() {
            return $this->name;
        }
        function GetPrice() {
            return $this->price;
        }
    }


   function Diaplay() {
       ?>
       <h1>Product List</h1>
       <dl>
  
	</dl>

       <?php 
   }
?>